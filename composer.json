{
  "name": "dragonbite/backend",
  "description": "Backend PHP para DragonBite App",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "ext-curl": "*"
  },
  "autoload": {
    "psr-4": {
      "DragonBite\\": "src/"
    }
  },
  "config": {
    "platform-check": false
  },
  "scripts": {
    "post-install-cmd": [
      "echo 'DragonBite Backend instalado'"
    ]
  }
}